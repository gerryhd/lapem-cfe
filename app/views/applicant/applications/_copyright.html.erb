        <md-input-container class="md-block" flex-gt-sm>
          <label>Primigenia o derivada</label>
          <div ng-if="application.copyright.is_derivated">Derivada</div>
          <div ng-if="!application.copyright.is_derivated">Primigenia</div>
        </md-input-container>

        <md-input-container class="md-block" flex-gt-sm ng-if="application.copyright.is_derivated == 1">
          <label>Tipo de derivación</label>
          <div> {{ application.copyright.derivation_type.name }} </div>
        </md-input-container>
      </div>


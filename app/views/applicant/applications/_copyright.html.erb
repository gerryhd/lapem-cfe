          <md-input-container class="md-block" flex-gt-sm>
            <label>Titulo</label>
            <div>{{data_copyright.title}}</div>
          </md-input-container>
          <md-input-container class="md-block" flex-gt-sm>
            <label>Autor</label>
            <div>{{data_copyright.author}}</div>
          </md-input-container>
        </div>

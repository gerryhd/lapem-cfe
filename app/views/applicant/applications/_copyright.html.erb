          <label>Sintesis</label>
          <div>{{application.copyright.summary}}</div>
          </md-input-container>
      </div>


      <div layout-gt-sm="row">
        <md-input-container class="md-block" flex-gt-sm>
          <label>Rama</label>
          <div> {{ application.copyright.copyright_branch.name }} </div>
        </md-input-container>

        <div class="md-block" flex-gt-sm>
          <label>¿Se ha dado a conocer?</label>
          <div ng-if="application.copyright.known_public">Sí</div>
          <div ng-if="!application.copyright.known_public">No</div>
        </div>

        <div class="md-block" ng-if="application.copyright.known_public == 1" flex-gt-sm>
          <label>
            <small>Fecha de publicación:</small>
          </label>
          <div> {{ application.copyright.publication_date | date:"dd/MM/yyyy" }} </div>
        </div>

      </div>

